<!--pages/alarmRelease/alarmRelease.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">解除警报成功</view>
            <view class="weui-msg__desc">设备警报已经解除，请点击按钮返回地图界面</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap='backToMap'>返回地图</button>
               
            </view>
        </view>

    </view>
</view>